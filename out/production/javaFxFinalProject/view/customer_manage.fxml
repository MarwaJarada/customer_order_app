<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="customerManage" prefHeight="360.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ManageCustomerController">
   <children>
      <VBox layoutX="5.0" layoutY="6.0" prefHeight="360.0" prefWidth="330.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="96.0" prefWidth="330.0" text="Manage Customer Menu" />
            <Button mnemonicParsing="false" onAction="#openAddCustomerView" prefHeight="30.0" prefWidth="190.0" text="Add Customer" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="80.0" layoutY="106.0" mnemonicParsing="false" onAction="#openUpdateCustomerView" prefHeight="30.0" prefWidth="190.0" text="Update Customer ">
               <VBox.margin>
                  <Insets left="70.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#openUpdateCutomerView" prefHeight="30.0" prefWidth="190.0" text="Delete Customer">
               <VBox.margin>
                  <Insets left="70.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#mainMenu" prefHeight="30.0" prefWidth="190.0" text="Main Menu">
               <VBox.margin>
                  <Insets left="70.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
